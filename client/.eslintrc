{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "rules": {
    "class-methods-use-this": [
      "error",
      {
        "exceptMethods": ["setObserver", "setEvent", "template", "didRender"]
      }
    ]
  }
}
